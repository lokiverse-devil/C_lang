#include <stdio.h>
#include<string.h>
int main() {
    char str[10];
    printf("Enter a string \n");
    scanf("%s",str);
    int length=strlen(str);
    for(int i=length-1;i>=0;i--){
        printf("%c",str[i]);
    }
    return 0;
}
//What this will do
//Suppose string is ="hello";
//It returns "olleh"
