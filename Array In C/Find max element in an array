#include <stdio.h>
int main() {
    int arr[5];
    int max=0;
    printf("Enter array elements\n");
    for(int i=0;i<5;i++){
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<5;i++){
        if(max<arr[i]){
            max=arr[i];
        }
    }
    printf("Maximum array element = %d",max);
    return 0;
}
//What this will do
//Suppose array =[1,2,3,4,5];
//Returns 5 as the max element
